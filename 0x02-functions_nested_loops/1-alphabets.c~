#include "main.h"
#include <unistd.h>
/**
*__putchar- Prints a character to the standard output
*@c: The character to be printed
*
*Return: On success,  returns the charcater printed. On error, returns -1.
*/
int __putchar(char c)
{
  return (write(1, &c, 1));
}
/***main - Entry point of the program.

